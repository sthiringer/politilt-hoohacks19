{
 "manifest_version": 2,
 "name": "Slant",
 "description": "Deciphering political bias one article at a time.",
 "version": "1.00",
 "author": "Jack Morris, Steve Thiringer, James Childress",
 "web_accessible_resources": [
 	"SlantLogo.png",
 	"logo.svg"
 ],
 "content_scripts": [
 	{
 		"matches": [
 			"<all_urls>"

 		],
 		"js": [ "jquery-3.3.1.js", "content.js" ]
 	}

 ],
 "background": {
 	"scripts": [ "jquery-3.3.1.js", "background.js" ],
	"css": ["bootstrap.css"]
 },
 "permissions": ["tabs"],
"browser_action": {
   "default_title": "Have a good day",
   "default_popup": "popup.html",
   "default_icon": "favicon-16x16.png"
  }
}